                <a target="_blank" href="https://www.youtube.com/channel/UCosXctaTYxN4YPIvI5Fpcrw"><svg
                        xmlns="http://www.w3.org/2000/svg" width="31" height="21" viewBox="0 0 31 21">
                        <path fill="black" fill-rule="nonzero"
                            d="M28.88 2.755c-.797-1.4-1.662-1.657-3.424-1.755C23.696.882 19.272.833 15.44.833c-3.84 0-8.266.05-10.024.165-1.758.1-2.625.356-3.429 1.757C1.165 4.152.743 6.559.743 10.797V10.81c0 4.22.422 6.645 1.243 8.028.804 1.399 1.67 1.653 3.427 1.77 1.76.102 6.186.162 10.026.162 3.833 0 8.258-.06 10.02-.16 1.76-.117 2.626-.371 3.423-1.77.828-1.383 1.247-3.809 1.247-8.028v-.01-.005c0-4.239-.419-6.646-1.25-8.043zM11.762 16.24V5.365l9.184 5.437-9.184 5.438z" />
                    </svg></a>


            </div>
            <div class="header-container-right">
                <div class="coke-imgs">
                    <a href="#zero"><img class="coke-zero-img" src="img/coke-zero.png" alt="Lata de Coca Zero"></a>
                    <a href="#original"><img class="coke-classic-img" src="img/coke-classic.png"
                            alt="Lata de Coca Original"></a>
                    <a href="#light"><img class="coke-light-img" src="img/coke-light.png" alt="Lata de Coca Light"></a>
                </div>
            </div>


            <%- include('./partials/faq') %> 
            <a class="click-box" href="#nav">
                <div data-aos="fade-up" data-aos-duration="500" class="go-back">


                    <div class="go-back-container">
                        <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" fill="currentColor"
                            class="bi bi-arrow-up-circle-fill" viewBox="0 0 16 16">
                            <path
                                d="M16 8A8 8 0 1 0 0 8a8 8 0 0 0 16 0zm-7.5 3.5a.5.5 0 0 1-1 0V5.707L5.354 7.854a.5.5 0 1 1-.708-.708l3-3a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1-.708.708L8.5 5.707V11.5z" />
                        </svg>
                        <h4>Volver al inicio</h4>
                    </div>

                </div>
            </a>
        </div>

    </header>

    <!-- Main -->

    <main id="news">

        <div class="main-container">

            <div class="main-text">
                <h2>Mira nuestras <i style="font-family: Lobster; font-size: 40px;">novedades</i></h2>
            </div>

            <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
                <div class="carousel-indicators">
                    <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0"
                        class="active" aria-current="true" aria-label="Slide 1"></button>
                    <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1"
                        aria-label="Slide 2"></button>
                    <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2"
                        aria-label="Slide 3"></button>
                </div>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img src="img/advert.jpg" class="d-block w-100" alt="Nueva Coca-Cola Zero">
                    </div>
                    <div class="carousel-item">
                        <img src="img/advert-2.jpg" class="d-block w-100" alt="Cocas personalizadas">
                    </div>
                    <div class="carousel-item">
                        <img src="img/advert-3.jpg" class="d-block w-100" alt="Papa Noel tomando Coca-Cola">
                    </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators"
                    data-bs-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators"
                    data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                </button>
            </div>

            <div class="curve">
                <svg data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 120"
                    preserveAspectRatio="none">
                    <path
                        d="M985.66,92.83C906.67,72,823.78,31,743.84,14.19c-82.26-17.34-168.06-16.33-250.45.39-57.84,11.73-114,31.07-172,41.86A600.21,600.21,0,0,1,0,27.35V120H1200V95.8C1132.19,118.92,1055.71,111.31,985.66,92.83Z"
                        class="shape-fill"></path>
                </svg>
            </div>

            <div class="curve-2">
                <svg data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 120"
                    preserveAspectRatio="none">
                    <path
                        d="M321.39,56.44c58-10.79,114.16-30.13,172-41.86,82.39-16.72,168.19-17.73,250.45-.39C823.78,31,906.67,72,985.66,92.83c70.05,18.48,146.53,26.09,214.34,3V0H0V27.35A600.21,600.21,0,0,0,321.39,56.44Z"
                        class="shape-fill"></path>
                </svg>
            </div>
        </div>
    </main>

    <!-- Sections -->

    <section class="coke-original">
        <div id="original" class="sec-container-coke">
            <div class="sec-container-left">
                <img src="img/Coca-Cola-Original.png" alt="Logo de Coca-Cola Zero">
            </div>
            <div class="sec-container-right">
                <% original.forEach(producto=> { %>

                    <div class="sec-card zero">

                        <img src="img/<%= producto.image %>" alt="<%= producto.description %>">
                        <hr class="card-break">
                        <h3>
                            <%= `${producto.name} ${producto.quantity} ${producto.type}` %>
                        </h3>
                        <span style="color: black; font-size: 20px;">
                            ARS$ <%= producto.price %>
                                </p>
                                <a href="/comprar/<%= producto._id  %> ">Comprar</a>
                    </div>

                    <% }); %>
            </div>
        </div>
    </section>


    <section class="coke-zero">
        <div id="zero" class="sec-container-coke">
            <div class="sec-container-left">
                <img src="img/Coca-Cola-Zero.png" alt="Logo de Coca-Cola Zero">
            </div>
            <div class="sec-container-right">

                <% zero.forEach(producto=> { %>

                    <div class="sec-card zero">

                        <img src="img/<%= producto.image %>" alt="<%= producto.description %>">
                        <hr class="card-break">
                        <h3>
                            <%= `${producto.name} ${producto.quantity} ${producto.type}` %>
                        </h3>
                        <span style="color: black; font-size: 20px;">
                            ARS$ <%= producto.price %>
                                </p>
                                <a href="/comprar/<%= producto._id %> ">Comprar</a>
                    </div>

                    <% }); %>
            </div>
        </div>
    </section>

    <section class="coke-light">
        <div id="light" class="sec-container-coke">

            <div class="sec-container-left">
                <img src="img/Coca-Cola-Light.png" alt="Logo de Coca-Cola Light">
            </div>

            <div class="sec-container-right">

                <% light.forEach(producto=> { %>

                    <div class="sec-card zero">

                        <img src="img/<%= producto.image %>" alt="<%= producto.description %> ">
                        <hr class="card-break">
                        <h3>
                            <%= `${producto.name} ${producto.quantity} ${producto.type}` %>
                        </h3>
                        <span style="color: black; font-size: 20px;">
                            ARS$ <%= producto.price %>
                                </p>
                                <a href="/comprar/<%= producto._id  %> ">Comprar</a>
                    </div>

                    <% }); %>
            </div>

        </div>
        </div>
    </section>

    <section>
        <div id="otras-marcas" class="more-brands">
            <h2>Otras <i style="font-family: Lobster; font-size: 35px;">marcas</i></h2>
            <a target="_blank" href="https://www.fantalatinamerica.com/ar/es/home"><img src="img/fanta-logo.png"
                    alt="Logo de Fanta"></a>
            <a target="_blank" href="https://www.cocacoladeargentina.com.ar/marcas/crush"><img src="img/crush-logo.png"
                    alt="Logo de Crush"></a>
            <a target="_blank" href="https://www.spritelatinamerica.com/ar/es/home"><img src="img/sprite-logo.png"
                    alt="Logo de Sprite"></a>
            <a target="_blank" href="https://www.cocacoladeargentina.com.ar/marcas/cepita"><img
                    src="img/cepita-logo.png" alt="Logo de Cepita"></a>
        </div>
    </section>

    <!-- Footer -->

    <%- include('./partials/footer') %>

    <!-- Scripts -->

    <script src="/js/main.js"></script>
    <%- include('./partials/scripts') %>

</body>

</html>